{
  "name": "@real-estate/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:generate": "node ./scripts/db-generate-safe.mjs",
    "db:generate:direct": "prisma generate --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:seed": "prisma db execute --schema prisma/schema.prisma --file prisma/seed.sql"
  },
  "exports": {
    ".": "./src/index.ts",
    "./crm": "./src/crm.ts",
    "./tenants": "./src/tenants.ts",
    "./website-config": "./src/website-config.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@real-estate/types": "*"
  },
  "devDependencies": {
    "prisma": "^6.16.3"
  },
  "types": "./src/index.ts"
}
