{
  "name": "@real-estate/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:generate": "node ./scripts/db-generate-safe.mjs",
    "db:generate:sample": "node ./scripts/db-generate-reliability-sample.mjs",
    "db:generate:direct": "node ./scripts/db-generate-direct.mjs",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "db:seed": "prisma db execute --schema prisma/schema.prisma --file prisma/seed.sql"
  },
  "exports": {
    ".": "./src/index.ts",
    "./admin-audit": "./src/admin-audit.ts",
    "./control-plane": "./src/control-plane.ts",
    "./crm": "./src/crm.ts",
    "./tenants": "./src/tenants.ts",
    "./transactions": "./src/transactions.ts",
    "./website-config": "./src/website-config.ts",
    "./integrations": "./src/integrations.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@real-estate/types": "*"
  },
  "devDependencies": {
    "prisma": "^6.16.3"
  },
  "types": "./src/index.ts"
}
